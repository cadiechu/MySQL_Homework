n\r\nDELIMITER ;\r\n\r\nDELIMITER //\r\n\r\nCREATE PROCEDURE DeleteProduct(\r\n    IN productId INT\r\n)\r\nBEGIN\r\nDELETE\r\nFROM Products\r\nWHERE id = productId;\r\nEND //\r\n\r\nDELIMITER ;
